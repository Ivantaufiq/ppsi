<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="6dp"
    android:orientation="vertical"
    tools:context=".Home.CariProyekActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:clipToPadding="false"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:layout_width="185dp"
                    android:layout_height="280dp"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="10dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="@drawable/budidayalele">
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:layout_margin="2dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:textStyle="bold"
                                android:layout_marginTop="5dp"
                                android:text="@string/lele"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary" />
                            <Button
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:onClick="pindahlele"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="left"
                                android:textAlignment="center"
                                android:background="@drawable/round2"
                                android:textColor="@color/putih"
                                android:text="@string/slgkpny"
                                android:textAllCaps="false" />
                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="185dp"
                    android:layout_height="280dp"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="10dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="@drawable/budidayanila">

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:layout_margin="2dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:textStyle="bold"
                                android:layout_marginTop="5dp"
                                android:text="@string/nila"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary" />
                            <Button
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="left"
                                android:textAlignment="center"
                                android:background="@drawable/round2"
                                android:textColor="@color/putih"
                                android:text="@string/slgkpny"
                                android:onClick="pindahnila"
                                android:textAllCaps="false" />
                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:clipToPadding="false"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:layout_width="185dp"
                    android:layout_height="280dp"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="10dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="@drawable/budidayapatin"></LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:layout_margin="2dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:textStyle="bold"
                                android:layout_marginTop="5dp"
                                android:text="@string/patin"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary" />
                            <Button
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="left"
                                android:textAlignment="center"
                                android:background="@drawable/round2"
                                android:textColor="@color/putih"
                                android:text="@string/slgkpny"
                                android:onClick="pindahpatin"
                                android:textAllCaps="false" />
                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="185dp"
                    android:layout_height="280dp"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="10dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="@drawable/budidayaikanmas"></LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:layout_margin="2dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:textStyle="bold"
                                android:layout_marginTop="5dp"
                                android:text="@string/ikanmas"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary" />
                            <Button
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="left"
                                android:textAlignment="center"
                                android:background="@drawable/round2"
                                android:textColor="@color/putih"
                                android:text="@string/slgkpny"
                                android:onClick="pindahmas"
                                android:textAllCaps="false" />
                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:clipToPadding="false"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:layout_width="185dp"
                    android:layout_height="280dp"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="10dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="@drawable/budidayagabus"></LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:layout_margin="2dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:textStyle="bold"
                                android:layout_marginTop="5dp"
                                android:text="@string/gabus"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary" />
                            <Button
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="left"
                                android:textAlignment="center"
                                android:background="@drawable/round2"
                                android:textColor="@color/putih"
                                android:text="@string/slgkpny"
                                android:onClick="pindahgabus"
                                android:textAllCaps="false" />
                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="185dp"
                    android:layout_height="280dp"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="10dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="@drawable/budidayaleleterpal">

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:layout_margin="2dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:textStyle="bold"
                                android:layout_marginTop="5dp"
                                android:text="@string/lele2"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary" />
                            <Button
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="left"
                                android:textAlignment="center"
                                android:background="@drawable/round2"
                                android:textColor="@color/putih"
                                android:text="@string/slgkpny"
                                android:onClick="pindahlele2"
                                android:textAllCaps="false" />
                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:clipToPadding="false"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:layout_width="185dp"
                    android:layout_height="280dp"
                    android:layout_margin="5dp"
                    app:cardCornerRadius="10dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:background="@drawable/budidayamujair">
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:layout_margin="2dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:textStyle="bold"
                                android:layout_marginTop="5dp"
                                android:text="@string/mujair"/>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary" />
                            <Button
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginLeft="2dp"
                                android:layout_gravity="left"
                                android:textAlignment="center"
                                android:background="@drawable/round2"
                                android:textColor="@color/putih"
                                android:text="@string/slgkpny"
                                android:onClick="pindahmujair"
                                android:textAllCaps="false" />
                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>